}

//Below is the table cotton_on_call

model cotton_on_call {
    record_id               String   @id @default(cuid())
    date                    DateTime @db.Date
    season_week             Int?
    week                    Int
    season                  String
    october_sales           Int?
    december_sales          Int?
    march_sales             Int?
    may_sales               Int?
    july_sales              Int?
    total_on_call_sales     Int?
